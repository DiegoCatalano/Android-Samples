<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="catalano.demoapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".video.VideoActivity" />
        <activity android:name=".music.MusicActivity" />
        <activity android:name=".basic.BasicActivity" />
        <activity android:name=".sensors.SensorActivity" />
        <activity android:name=".animation.AnimationActivity" />
        <activity android:name=".activity.ActActivity" />
        <activity android:name=".activity.AnotherActivity"></activity>
    </application>

</manifest>